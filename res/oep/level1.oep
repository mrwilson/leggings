		<object name="red" image="redblock.png" width ="32" height="19" originX="0" originY="2" />
		<object name="blue" image="blueblock.png" width ="32" height="19" originX="0" originY="2" />
		<object name="green2" image="greenblock2.png" width="64" height="19" originX="0" originY="2" />
		<object name="grey2" image="greyblock2.png" width="64" height="19" originX="0" originY="2" />
		<object name="yellow2" image="yellowblock2.png" width="64" height="19" originX="0" originY="2" />
		<object name="red2" image="redblock2.png" width="64" height="19" originX="0" originY="2" />
		<object name="blue2" image="blueblock2.png" width="64" height="19" originX="0" originY="2" />
		<object name="green3" image="greenblock3.png" width="96" height="19" originX="0" originY="2" />
